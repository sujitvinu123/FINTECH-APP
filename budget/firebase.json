{
  "hosting": {
    "public": "build/web",
    "predeploy": "flutter build web --release --web-renderer canvaskit --no-tree-shake-icons",
    "site":"budget-track",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
